{
  "name": "snowpack-starter",
  "version": "1.0.0",
  "description": "Snowpack starter template",
  "main": "index.js",
  "scripts": {
    "prepare": "snowpack",
    "start": "snowpack dev",
    "build": "snowpack build --clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davi-mbatista/snowpack-starter.git"
  },
  "author": "@davi-mbatista",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davi-mbatista/snowpack-starter/issues"
  },
  "dependencies": {
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "preact": "10.5.4"
  },
  "devDependencies": {
    "@prefresh/snowpack": "2.0.1",
    "@snowpack/app-scripts-react": "1.12.2",
    "@snowpack/plugin-dotenv": "2.0.3",
    "@snowpack/plugin-react-refresh": "2.3.1",
    "@snowpack/plugin-typescript": "1.0.1",
    "@snowpack/plugin-webpack": "2.0.15",
    "@types/react": "16.9.51",
    "@types/react-dom": "16.9.8",
    "@types/snowpack-env": "2.3.0",
    "snowpack": "2.13.3",
    "typescript": "4.0.3"
  },
  "snowpack": {
    "mount": {
      "public": "/",
      "src": "/_dist_"
    },
    "plugins": [
      "@snowpack/plugin-dotenv",
      "@snowpack/plugin-react-refresh",
      "@snowpack/plugin-typescript",
      "@snowpack/plugin-webpack"
    ],
    "alias": {
      "react": "preact/compat",
      "react-dom": "preact/compat"
    },
    "installOptions": {
      "installTypes": true
    }
  }
}
